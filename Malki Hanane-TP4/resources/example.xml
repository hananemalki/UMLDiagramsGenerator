<?xml version="1.0" encoding="UTF-8"?>

<project>
    <package name="org.mql.java.reflection.examples">

        <!-- Classe Commande -->
        <class>
            <name>Commande</name>
            <fields>
                <field>
                    <name>id</name>
                    <type>String</type>
                    <modifier>private</modifier>
                </field>
                <field>
                    <name>date</name>
                    <type>String</type>
                    <modifier>private</modifier>
                </field>
                <field>
                    <name>produits</name>
                    <type>List&lt;Produit&gt;</type>
                    <modifier>private</modifier>
                </field>
                <field>
                    <name>utilisateur</name>
                    <type>Utilisateur</type>
                    <modifier>private</modifier>
                </field>
            </fields>
            <methods>
                <method>
                    <name>ajouterProduit</name>
                    <returnType>void</returnType>
                    <modifier>public</modifier>
                    <parameters>
                        <parameter>
                            <name>produit</name>
                            <type>Produit</type>
                        </parameter>
                    </parameters>
                </method>
                <method>
                    <name>getId</name>
                    <returnType>String</returnType>
                    <modifier>public</modifier>
                </method>
                <method>
                    <name>setId</name>
                    <returnType>void</returnType>
                    <modifier>public</modifier>
                    <parameters>
                        <parameter>
                            <name>id</name>
                            <type>String</type>
                        </parameter>
                    </parameters>
                </method>
                <method>
                    <name>getDate</name>
                    <returnType>String</returnType>
                    <modifier>public</modifier>
                </method>
                <method>
                    <name>setDate</name>
                    <returnType>void</returnType>
                    <modifier>public</modifier>
                    <parameters>
                        <parameter>
                            <name>date</name>
                            <type>String</type>
                        </parameter>
                    </parameters>
                </method>
                <method>
                    <name>getProduits</name>
                    <returnType>List&lt;Produit&gt;</returnType>
                    <modifier>public</modifier>
                </method>
                <method>
                    <name>setProduits</name>
                    <returnType>void</returnType>
                    <modifier>public</modifier>
                    <parameters>
                        <parameter>
                            <name>produits</name>
                            <type>List&lt;Produit&gt;</type>
                        </parameter>
                    </parameters>
                </method>
                <method>
                    <name>getUtilisateur</name>
                    <returnType>Utilisateur</returnType>
                    <modifier>public</modifier>
                </method>
                <method>
                    <name>setUtilisateur</name>
                    <returnType>void</returnType>
                    <modifier>public</modifier>
                    <parameters>
                        <parameter>
                            <name>utilisateur</name>
                            <type>Utilisateur</type>
                        </parameter>
                    </parameters>
                </method>
            </methods>
            <relations>
                <relation>
                    <type>aggregation</type>
                    <target>Produit</target>
                </relation>
                <relation>
                    <type>association</type>
                    <target>Utilisateur</target>
                </relation>
            </relations>
        </class>

